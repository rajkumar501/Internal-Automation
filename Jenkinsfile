pipeline {
    agent{
        kubernetes{
            label 'jenkins-jenkins-slave'
            defaultContainer 'terraform-az'
        }
    }

    stages {
        stage('Build') {
            steps {
                echo 'Building..'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing..'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying....'
            }
        }
    }
}
